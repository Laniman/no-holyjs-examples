{
  "name": "holyjs-examples",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install && yarn build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "stylelint": "stylelint \"**/*.css\"",
    "pretty": "yarn format && yarn lint --fix && yarn stylelint --fix",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6010"
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --no-error-on-unmatched-pattern --fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toggle": "^1.0.3",
    "@tanstack/react-query": "^5.17.10",
    "axios": "^1.6.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.309.0",
    "next": "14.0.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.0.4",
    "@siberiacancode/eslint": "^1.0.3",
    "@siberiacancode/prettier": "^1.0.1",
    "@siberiacancode/stylelint": "^1.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "husky": "^8.0.3",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.2.0",
    "mock-config-server": "^3.3.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "storybook": "^7.6.6",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  }
}
